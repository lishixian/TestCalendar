<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.tea.testcalendar">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity
            android:name=".MainActivity"
            android:label="@string/app_name">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <activity
            android:name=".home.BasicActivity"
            android:label="@string/title_activity_basic" >
        </activity>


        <!-- remind -->
        <activity
            android:name=".remind.RemindBACActivity"
            android:theme="@style/RemindBACTheme"
            android:screenOrientation="portrait"
            android:configChanges="orientation|screenSize|mcc|mnc|keyboard|keyboardHidden|navigation|uiMode|fontScale">

        </activity>

        <activity
            android:name=".remind.RemindBACAddActivity"
            android:theme="@style/RemindBACTheme"
            android:screenOrientation="portrait"
            android:configChanges="orientation|screenSize|mcc|mnc|keyboard|keyboardHidden|navigation|uiMode|fontScale">

        </activity>
    </application>

</manifest>